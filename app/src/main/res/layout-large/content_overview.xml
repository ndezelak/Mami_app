<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
     tools:context="com.nejc.mamiapp.activities.OverviewActivity"
   >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Vrsta Smene"
        android:id="@+id/textTabelaLevo"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="30dp"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Display1"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Število ur"
        android:id="@+id/textTabelaDesno"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="30dp"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Display1"
        android:layout_marginRight="30dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Kuhinja Dopoldan"
        android:id="@+id/textKuhinjaDop"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textTabelaLevo"
        android:layout_marginTop="60dp" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:id="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/textKuhinjaDop"
        android:layout_marginTop="20dp"
        android:src="@drawable/rectangle"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="2"
        android:text="5"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/editTextKuhinjaDop"
        android:layout_below="@id/textKuhinjaDop"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_marginRight="30dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Kuhinja Popoldan"
        android:id="@+id/textKuhinjaPop"
       android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="10dp" />

    <ImageView
        android:layout_width="600dp"
        android:layout_height="50dp"
        android:id="@+id/imageView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textKuhinjaPop"
        android:src="@drawable/rectangle"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:inputType="numberSigned"
        android:ems="2"
        android:text="5"
        android:id="@+id/editTextKuhinjaPop"
        android:layout_below="@id/textKuhinjaPop"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_marginRight="30dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Šank Dopoldan"
        android:id="@+id/textŠankDop"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="10dp" />

    <ImageView
        android:layout_width="600dp"
        android:layout_height="50dp"
        android:id="@+id/imageView3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textŠankDop"
        android:src="@drawable/rectangle"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:inputType="numberSigned"
        android:ems="2"
        android:text="5"
        android:id="@+id/editTextŠankDop"
        android:layout_below="@id/textŠankDop"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_marginRight="30dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Šank Popoldan"
        android:id="@+id/textŠankPop"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="10dp" />

    <ImageView
        android:layout_width="600dp"
        android:layout_height="50dp"
        android:id="@+id/imageView4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textŠankPop"
        android:src="@drawable/rectangle"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:inputType="numberSigned"
        android:ems="2"
        android:text="5"
        android:id="@+id/editTextŠankPop"
        android:layout_below="@id/textŠankPop"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_marginRight="30dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Prosto"
        android:id="@+id/textProsto"
        android:layout_below="@+id/imageView4"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="10dp" />

    <ImageView
        android:layout_width="600dp"
        android:layout_height="50dp"
        android:id="@+id/imageView5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textProsto"
        android:src="@drawable/rectangle"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:inputType="numberSigned"
        android:ems="2"
        android:text="5"
        android:id="@+id/editTextProsto"
        android:layout_below="@id/textProsto"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_marginRight="30dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Dopust"
        android:id="@+id/textDopust"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="10dp" />

    <ImageView
        android:layout_width="600dp"
        android:layout_height="50dp"
        android:id="@+id/imageView6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_below="@id/textDopust"
        android:src="@drawable/rectangle"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:inputType="numberSigned"
        android:ems="2"
        android:text="5"
        android:id="@+id/editTextDopust"
        android:layout_below="@id/textDopust"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_marginRight="30dp"
        />


</RelativeLayout>
